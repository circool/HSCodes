<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane hgap="14.0" maxHeight="+Infinity" maxWidth="+Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="20.0"
          xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="ru.tsurkanenko.vladimir.hscodes.mvc.ControllerHelp">
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity" minWidth="-Infinity"/>
    <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="+Infinity" minWidth="-Infinity"/>
  </columnConstraints>
  <padding>
    <Insets bottom="14.0" left="14.0" right="14.0" top="14.0"/>
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="+Infinity" minHeight="-Infinity" valignment="CENTER" vgrow="ALWAYS"/>
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="NEVER"/>
  </rowConstraints>
  <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="0"
             GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="TOP">
    <!-- place holder -->
  </ImageView>
  <VBox maxHeight="+Infinity" maxWidth="+Infinity" minHeight="-Infinity" prefWidth="400.0" spacing="7.0"
        GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.vgrow="ALWAYS">
    <Label fx:id="helpMessageLabel" text="message" wrapText="true">
      <font>
        <Font name="System Bold" size="13.0"/>
      </font>
    </Label>
    <Label fx:id="helpDetailsLabel" text="details" wrapText="true">
      <font>
        <Font size="12.0"/>
      </font>
    </Label>
  </VBox>
  <HBox maxHeight="-Infinity" maxWidth="+Infinity" minHeight="-Infinity" minWidth="-Infinity" GridPane.columnIndex="1"
        GridPane.rowIndex="1">
    <HBox alignment="CENTER">
      <HBox.margin>
        <Insets/>
      </HBox.margin>
    </HBox>
    <Pane maxWidth="+Infinity" HBox.hgrow="ALWAYS"/>
    <Button cancelButton="true" minWidth="80.0" mnemonicParsing="false"
            onAction="#helpCloseButtonOnAction" text="Close" HBox.hgrow="NEVER">
      <HBox.margin>
        <Insets/>
      </HBox.margin>
    </Button>

  </HBox>
</GridPane>
